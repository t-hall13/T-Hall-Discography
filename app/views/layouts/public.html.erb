<!DOCTYPE html>
<html>
	<head>
		<%= stylesheet_link_tag('application', :media => 'all') %>
	  <title>T Hall Discography | <%= "#{@page_title}" if @page_title %></title>
	  	  <%= javascript_include_tag('public') %>
	</head>
	<body>
		<div id="header">
			<h1>Tim Hall Discography<h1>
		</div>
		<!--before yield-->
		<div id="main">
				<div id="navigation">
				<%= render(:partial => 'layouts/navigation_2', :locals => {:artists => @artists}) %>
				</div>
			<div id="content">
				<%= yield %>
			</div>
			<div class="clear"></div>
		<!--after yield-->
		</div>
		<div id="footer">
			<p id="copyright">&copy; 2014 T.Hall</p>
		</div>
	</body>
</html>